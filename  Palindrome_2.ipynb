   "source": [
    "def palindrome_check(num):\n",
    " ns=str(num)\n",
    " return ns == ns[::-1]\n",
    "\n",
    "number= int(input(\"enter a number to check palindrome or not\"))\n",
    "if palindrome_check(number):\n",
    "    print(\"yes palindrome\")\n",
    "else :\n",
    "    print(\"not palindrome\")\n",
    "\n"
   ]
